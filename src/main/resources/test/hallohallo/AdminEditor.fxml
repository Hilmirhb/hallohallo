<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="test.hallohallo.AdminEditor">

    <children>
        <ListView fx:id="fxTourList" layoutX="63.0" layoutY="127.0" prefHeight="310.0" prefWidth="295.0" />
        <TextField fx:id="fxSearchByText" layoutX="63.0" layoutY="88.0" prefHeight="25.0" prefWidth="130.0" promptText="Search by name" />
        <ChoiceBox fx:id="fxChoiceBox" layoutX="275.0" layoutY="88.0" prefHeight="25.0" prefWidth="83.0" />
        <Button fx:id="fxSearchbutton" layoutX="204.0" layoutY="88.0" mnemonicParsing="false" text="Search" />
        <Button fx:id="fxNewTourButton" layoutX="260.0" layoutY="448.0" mnemonicParsing="false" text="Create new tour" onAction="#HandleNewTourButton"/>
        <Button fx:id="fxLogoutButton" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#handleLogoutButtonClick" text="Log Out" />
      <Label layoutX="172.0" layoutY="63.0" prefHeight="17.0" prefWidth="73.0" text=" Your  Tours" />
        <ListView fx:id="fxReservationList" layoutX="433.0" layoutY="127.0" prefHeight="141.0" prefWidth="295.0" />
      <Label layoutX="525.0" layoutY="93.0" text="Recent Reservations" />
      <Label fx:id="HostName" layoutX="294.0" layoutY="36.0" prefHeight="17.0" prefWidth="192.0" text="Host name" textAlignment="CENTER" />
        <ListView fx:id="fxReviewList" layoutX="433.0" layoutY="296.0" prefHeight="141.0" prefWidth="295.0" />
        <Label layoutX="537.0" layoutY="274.0" text="Recent Reviews" />
        <Button fx:id="fxViewReviewButton" layoutX="433.0" layoutY="448.0" mnemonicParsing="false" text="View Review" onAction="#HandleViewReviewButton"/>
        <Button fx:id="fxViewReservationButton" layoutX="617.0" layoutY="448.0" mnemonicParsing="false" text="View Reservation" onAction="#HandleViewReservationButton" />
        <Button fx:id="fxViewTourButton" layoutX="63.0" layoutY="448.0" mnemonicParsing="false" text="View Tour" onAction="#HandleViewTourButton"/>


    </children>

</AnchorPane>
